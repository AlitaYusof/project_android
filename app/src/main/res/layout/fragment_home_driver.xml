<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

    </data>

    <FrameLayout
        android:id="@+id/homeDriver"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Driver.HomeDriverFragment">


        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <Button
                android:id="@+id/btn_received_work"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="งานที่รับ"
                app:layout_constraintBottom_toTopOf="@+id/btn_getjob"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <Button
                android:id="@+id/btn_getjob"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="รับงาน"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/btn_received_work" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </FrameLayout>
</layout>